{
  "name": "oldskool-server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --build tsconfig.prod.json",
    "start": "node ./dist --env=production",
    "start:dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "@types"
    ],
    "ext": "ts",
    "exec": "ts-node ./src"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fast-glob": "^3.2.4",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.2",
    "morgan": "^1.10.0",
    "string-replace-async": "^2.0.0",
    "yaml": "^2.0.0-1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.2",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
